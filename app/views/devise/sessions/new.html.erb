<h2><%= t('.sign_in', :default => "Sign in") %></h2>

<%= simple_form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.submit t('.sign_in', :default => "Sign in"), :class => 'btn btn-primary' %>
<% end %>

<%= render "devise/shared/links" %>
